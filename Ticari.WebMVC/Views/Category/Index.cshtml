
﻿@model List<Category>
<<a asp-action="Create" class="btn btn-primary">Yeni Kayit</a>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Category Name</th>
            <th>Description</th>
        </tr>
    <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.Id</td>
                <td>@item.CategoryName</td>
                <td>@item.Description</td>

            </tr>
            }
    </tbody>
    </thead>
</table>
